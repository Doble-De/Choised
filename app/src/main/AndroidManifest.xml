<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dalealdado.dalealdado">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DadosActivity" />
        <activity android:name=".InfoActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".PersonajesActivity"
            android:label="@string/title_activity_personajes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".EnemigosNewActivity" />
        <activity android:name=".EnemigosShowActivity" />
        <activity android:name=".HeroesNewActivity" />
        <activity android:name=".HeroesShowActivity" />
        <activity android:name=".ShopActivity"></activity>
    </application>

</manifest>