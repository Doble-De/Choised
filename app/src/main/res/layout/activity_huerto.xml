<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/huerto">


    <ImageButton
        android:id="@+id/izquierda10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/izquierda"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.441" />

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/semilla1"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="70dp"
        android:background="@null"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/seed1"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:src="@drawable/seed1" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>


    <TextView
        android:id="@+id/textView4"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Calabaza"
        android:textColor="@android:color/background_dark"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/semilla1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="65dp"
        android:layout_height="32dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Patata"
        android:textColor="@android:color/background_dark"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/semilla1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="88dp"
        android:layout_height="32dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Guindilla"
        android:textColor="@android:color/background_dark"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/semilla1" />

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/semilla2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="70dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintStart_toEndOf="@+id/semilla1"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/seed2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:src="@drawable/seed2" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/semilla3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="70dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/semilla2"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/seed3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:src="@drawable/seed3" />
    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/hueco1"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="250dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/semilla1">

        <ImageView
            android:id="@+id/planta1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/creceplanta"
            android:visibility="invisible" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/hueco2"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="8dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintEnd_toStartOf="@+id/hueco3"
        app:layout_constraintStart_toEndOf="@+id/hueco1"
        app:layout_constraintTop_toBottomOf="@+id/semilla1">

        <ImageView
            android:id="@+id/planta2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/creceplanta"
            android:visibility="invisible" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/hueco3"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="20dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/semilla1">

        <ImageView
            android:id="@+id/planta3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/creceplanta"
            android:visibility="invisible" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/hueco4"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hueco1">

        <ImageView
            android:id="@+id/planta4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/creceplanta"
            android:visibility="invisible" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/hueco5"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintEnd_toStartOf="@+id/hueco6"
        app:layout_constraintStart_toEndOf="@+id/hueco4"
        app:layout_constraintTop_toBottomOf="@+id/hueco2">

        <ImageView
            android:id="@+id/planta5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/creceplanta"
            android:visibility="invisible" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    <com.jcminarro.roundkornerlayout.RoundKornerLinearLayout
        android:id="@+id/hueco6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="20dp"
        android:orientation="horizontal"
        app:corner_radius="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hueco3">

        <ImageView
            android:id="@+id/planta6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/creceplanta"
            android:visibility="invisible" />

    </com.jcminarro.roundkornerlayout.RoundKornerLinearLayout>

    </android.support.constraint.ConstraintLayout>